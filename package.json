  "scripts": {
    "drive:get": "./node_modules/.bin/selenium-standalone install",
    "drive": "./node_modules/.bin/selenium-standalone start",
    "start": "npm run dev",
    "dev": "node build/dev-server.js",
    "serve": "node build/prod-server.js",
    "build": "npm run build:worker && node build/build.js",
    "build:lib": "webpack --config build/webpack.lib.conf.js --progress --hide-modules",
    "build:worker": "webpack --config build/webpack.worker.conf.js --progress --hide-modules",
    "unit": "karma start --single-run",
    "unit:watch": "gulp",
    "e2e": "./node_modules/.bin/_mocha ./test/e2e/*.test.js --compilers js:babel-register --ui tdd --full-trace",
    "test": "npm run lint && npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
